<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dream.treevisualizer">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tree"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tree"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".BinarySearchTreeActivity" />
        <activity android:name=".AvlTreeActivity" />
        <activity android:name=".RedBlackTreeActivity" />
        <activity android:name=".CodeActivity" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".RBTDetails1" />
        <activity android:name=".AVLCode" />
        <activity android:name=".AVLDetails" />
        <activity android:name=".RBTCode"></activity>
    </application>

</manifest>